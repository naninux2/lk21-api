{
    "name": "lk21-api",
    "version": "1.0.0",
    "description": "Unofficial LK21 API",
    "main": "dist/index.js",
    "private": true,
    "_moduleAliases": {
        "@": "dist"
    },
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "serve": "concurrently \"npm run watch\" \"nodemon dist/index.js\"",
        "start": "node dist/index.js",
        "format": "prettier --write .",
        "test:redis": "node tests/redis.test.js",
        "cache": "node scripts/cache-cli.js",
        "docs:generate": "swagger-jsdoc -d src/config/swagger.ts src/routes/*.ts -o docs/swagger.json",
        "docs:serve": "npm run build && npm start",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:studio": "drizzle-kit studio",
        "db:setup": "bun scripts/setup-db.ts",
        "cli:create": "bun cli/api-keys-simple.ts create",
        "cli:list": "bun cli/api-keys-simple.ts list",
        "cli:show": "bun cli/api-keys-simple.ts show",
        "cli:revoke": "bun cli/api-keys-simple.ts revoke",
        "cli:update": "bun cli/api-keys-simple.ts update"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/febriadj/lk21-api.git"
    },
    "keywords": [
        "lk21-api",
        "movies-streaming",
        "web-scraping"
    ],
    "author": "Febriadji",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/febriadj/lk21-api/issues"
    },
    "homepage": "https://github.com/febriadj/lk21-api#readme",
    "devDependencies": {
        "@types/cheerio": "^0.22.31",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/morgan": "^1.9.4",
        "@types/node": "^18.15.3",
        "@types/pg": "^8.15.5",
        "@typescript-eslint/eslint-plugin": "^5.55.0",
        "@typescript-eslint/parser": "^5.55.0",
        "drizzle-kit": "^0.31.4",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.7.0",
        "eslint-plugin-prettier": "^4.2.1",
        "nodemon": "^2.0.21",
        "prettier": "^2.8.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "@inquirer/prompts": "^7.8.6",
        "@types/crypto-js": "^4.2.2",
        "@types/inquirer": "^9.0.9",
        "@types/redis": "^4.0.11",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.8",
        "axios": "^1.3.4",
        "chalk": "^5.6.2",
        "cheerio": "^1.0.0-rc.12",
        "cli-table3": "^0.6.5",
        "commander": "^14.0.1",
        "concurrently": "^8.2.0",
        "cors": "^2.8.5",
        "crypto-js": "^4.2.0",
        "dotenv": "^16.0.3",
        "drizzle-orm": "^0.44.5",
        "express": "^4.18.2",
        "inquirer": "^12.9.6",
        "module-alias": "^2.2.3",
        "morgan": "^1.10.0",
        "pg": "^8.16.3",
        "playwright": "^1.55.0",
        "redis": "^5.8.2",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1"
    }
}
